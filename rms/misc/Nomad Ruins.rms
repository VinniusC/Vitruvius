/* OBJECTS_GENERATION: Places units, buildings, and resources on the map */
<OBJECTS_GENERATION>
/* City wall generation */
create_object WALL 
{
	number_of_objects       9000		/* Maximum number of wall segments */
	terrain_to_place_on TEMP_TERRAIN	/* Places walls on TEMP_TERRAIN */
	set_gaia_object_only			    /* Walls belong to Gaia (neutral) */
	set_gaia_unconvertible			    /* Walls cannot be converted */
	actor_area              99 			/* Actor area ID 99 */
	actor_area_radius       4			/* 4-tile radius for actor area */
}
/* Regicide game mode - spawns KING when Regicide is active */
if REGICIDE
create_object KING
{	
	set_place_for_every_player  				/* Places KING for each player land */
	min_distance_to_players 			4		/* Minimum distance of KING generation from player origin */
	find_closest                 				/* Placed at closest valid location to land origin */
}
endif
/* Villager placement - automatic count based on civilization */
create_object VILLAGER
{
	set_place_for_every_player    				/* Places VILLAGER for each player land */
	min_distance_to_players 			3		/* Minimum distance of VILLAGER generation from player origin */
	max_distance_to_players 			5		/* Maximum distance of VILLAGER generation from player origin */
}	
/* Birds */
create_object BIRDS_A
{
	number_of_objects 					4			/* Number of BIRDS_A per group */
	number_of_groups            		3		    /* Number of BIRDS_A groups */
	group_variance						2           /* BIRDS_A randomly varied by up to the amount specified */	
	set_scaling_to_map_size							/* Scales object amount to map size */
}
create_object BIRDS_B
{
	number_of_objects 					4			/* Number of BIRD_B per group */
	number_of_groups            		2		    /* Number of BIRD_B groups */
	group_variance						2           /* BIRDS_B randomly varied by up to the amount specified */	
	set_scaling_to_map_size							/* Scales object amount to map size */
}
/* Castle Ruins Decoration */
create_object CASTLE_RUINS 
{
	number_of_objects           		1		    /* Number of CASTLE_RUINS per group */
	number_of_groups            		2		    /* Number of CASTLE_RUINS groups */
	set_scaling_to_map_size							/* Scales with map size */
	min_distance_group_placement 		3	        /* Minimum 3 tiles between castle ruins */
	set_gaia_object_only			        		/* Castle ruins belong to Gaia */
	terrain_to_place_on 			DLC_ROADGRAVEL	/* Places on DLC_ROADGRAVEL terrain */
	actor_area                  		9999	    /* Actor area ID 9999 */
	actor_area_radius           		8		    /* 8-tile radius for actor area */
	temp_min_distance_group_placement 	16			/* Avoids other castle ruins within 16 tiles */
	place_on_specific_land_id 			11		    /* Places on central city land (ID 11) */
}
create_object RUBBLE_1_X_1 
{
	number_of_objects 					3			/* 1-5 RUBBLE_1_X_1 per group */
	number_of_groups 					4			/* 4 groups total */
	group_variance 						2			/* RUBBLE_1_X_1 randomly varied by up to the amount specified */
	set_tight_grouping			            		/* RUBBLE_1_X_1 spawn close together */
	set_scaling_to_map_size			        		/* Scales with map size */
    make_indestructible								/* Makes object indestructible */
	find_closest									/* Places at closest valid location */
	actor_area_to_place_in 				9999		/* Must be placed within actor area 9999 */
}
/* Neutral Trade Workshop */
create_object T_WORKSHOP 
{
	number_of_objects 					1			/* Only 1 T_WORKSHOP */
	set_gaia_object_only			        		/* T_WORKSHOP belongs to Gaia */
	set_gaia_unconvertible			        		/* Cannot be converted */
	place_on_specific_land_id 			11		    /* Places on central city land (ID 11) */
    make_indestructible                     		/* Makes object indestructible */
	find_closest					        		/* Places at closest valid location */
	actor_area 							700			/* Actor area ID 700 */
	actor_area_radius 					4			/* 4-tile radius for actor area */
} 
/* Military Building Decoration */
create_object SIEGE_WORKSHOP
{
	number_of_objects 					1			/* 1 SIEGE_WORKSHOP per group */
	number_of_groups 					4			/* 4 groups total */
	set_scaling_to_map_size			        		/* Scales with map size */
	temp_min_distance_group_placement 	30			/* Avoids other SIEGE_WORKSHOP within 30 tiles */
	min_distance_to_players 			25		    /* Minimum 25 tiles from players */

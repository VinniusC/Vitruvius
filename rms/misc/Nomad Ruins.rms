/* ELEVATION_GENERATION: Create hills */
<ELEVATION_GENERATION>

create_elevation 5 								/* Sets max height allowed in create_elevation (0-16) */
{
	base_terrain 				LAYER_A			/* Terrain type to apply elevation */
	set_scale_by_size							/* Scales tile count by map size */
	number_of_tiles 			2048			/* Amount of elevated tiles */
	number_of_clumps 			(LENGTH / 8)	/* Amount of hills - LENGTH divided by 10 */
	enable_balanced_elevation					/* To keep elevation balanced across entirety of map */
}
create_elevation 2 								/* Sets max height allowed in create_elevation (0-16) */
{
	base_terrain 				DLC_ROADGRAVEL	/* Terrain type to apply elevation */
	set_scale_by_size							/* Scales tile count by map size */
	number_of_tiles 			512 			/* Amount of elevated tiles */
	number_of_clumps 			(LENGTH / 12)	/* Amount of hills - LENGTH divided by 10 */
	enable_balanced_elevation					/* To keep elevation balanced across entirety of map */
}


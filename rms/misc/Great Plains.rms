/* Water creation - multiple small lakes scattered across the map */
create_terrain VODA
{
	base_terrain 					LAYER_A			/* Terrain type to create new terrain from */
	land_percent 					3				/* How much VODA is present on terrain in percent */
	number_of_clumps 				1				/* Number of clumps to be created - multiple small lakes */
	set_scale_by_groups								/* Scales clumps by map size */	
	spacing_to_other_terrain_types 	3				/* Spacing to other terrains in tiles */
	set_avoid_player_start_areas 	(LENGTH / 5)
	height_limits					0 0				/* Limits height of water to base level */
	clumping_factor 				rnd(4,8)		/* How much WOODIES prefer to be together next to other tiles of the same clump. */
}

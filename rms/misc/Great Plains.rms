/* LAND_GENERATION: Creates large terrain areas including player starting areas */
<LAND_GENERATION>
base_terrain LAYER_A						/* Sets default terrain covering entire map */
/* Create player lands with open plains */
create_player_lands 						/* Creates a land for every player with specified parameters */
{
	generate_mode 1
	terrain_type	            LAYER_A  	/* Terrain type for player starting clearings */
	set_zone_by_team              			/* Team members share same zone - their lands can grow to touch each other */
	number_of_tiles 				1028 	/* Player land size in tiles */
	if LENGTH < 120
			circle_radius 			33 1	/* Positions players in circle: 33% radius from center, 0 variance */
			base_size               12		/* 12-tile square land origin size */
	elseif LENGTH 120
		if 2_PLAYER_GAME
			circle_radius 	rnd(32,34) 6	/* Positions players in circle: 32-34% radius from center, 6 variance */
			base_size               14		/* 14-tile square land origin size */
		else
			circle_radius 			34 1	/* Positions players in circle: 34% radius from center,	1 variance */
			base_size               14		/* 14-tile square land origin size */
		endif
	elseif LENGTH 144
		if 2_PLAYER_GAME
			circle_radius 	rnd(34,36) 8	/* Positions players in circle: 34-36% radius from center, 8 variance */
			base_size               16		/* 16-tile square land origin size */
		else
			circle_radius 			35 1	/* Positions players in circle: 35% radius from center, 1 variance */
			base_size               16		/* 16-tile square land origin size */
		endif
	elseif LENGTH 168	
		if 2_PLAYER_GAME
			circle_radius 	rnd(36,38) 8	/* Positions players in circle: 36-38% radius from center, 8 variance */
			base_size               18		/* 18-tile square land origin size */
		else
			circle_radius 			35 2	/* Positions players in circle: 35% radius from center, 2 variance */
			base_size               18		/* 18-tile square land origin size */
		endif
	elseif LENGTH 200	
		circle_radius 				36 3	/* Positions players in circle: 36% radius from center, 3 variance */
		base_size               	20		/* 20-tile square land origin size */
	elseif LENGTH 220	
		circle_radius 				37 4	/* Positions players in circle: 37% radius from center, 4 variance */
		base_size               	22		/* 22-tile square land origin size */
	elseif LENGTH 240	
		circle_radius 				38 5	/* Positions players in circle: 38% radius from center, 5 variance */
		base_size               	24		/* 24-tile square land origin size */
	elseif LENGTH 252	
		circle_radius 				39 6	/* Positions players in circle: 39% radius from center, 6 variance */
		base_size               	24		/* 24-tile square land origin size */
	elseif LENGTH > 252	
		circle_radius 				40 6	/* Positions players in circle: 40% radius from center, 6 variance */
		base_size               	24		/* 24-tile square land origin size */
	endif
	other_zone_avoidance_distance 	4    	/* 4-tile distance between player zones */
	set_circular_base             			/* Makes land origin circular instead of square */
}	

/* ELEVATION_GENERATION: Create rolling hills */
<ELEVATION_GENERATION>
create_elevation 3 							/* Sets max height allowed in create_elevation (0-16) */
{
	base_terrain 		LAYER_A				/* Terrain type to apply elevation */
	set_scale_by_size						/* Scales tile count by map size */
	number_of_tiles 	1024				/* Amount of elevated tiles */
	number_of_clumps 	(LENGTH / 3)		/* Amount of hills LENGTH divided by 3 */
	enable_balanced_elevation				/* To keep elevation balanced across entirety of map */
}

/* CLIFF_GENERATION: Create cliffs */
<CLIFF_GENERATION>
min_number_of_cliffs		(LENGTH / 16)	/* Minimum number of cliffs in map */	
max_number_of_cliffs 		(LENGTH / 10)	/* Maximum number of cliffs in map */
min_length_of_cliff 			4			/* Minimum length of a cliff */
max_length_of_cliff 			12			/* Maximum length of a cliff */
cliff_curliness 				30			/* Curliness of cliffs (0-100) */
min_distance_cliffs 			3			/* Minimum distance between cliffs */
min_terrain_distance 			1			/* Minimum distance between cliffs and water */


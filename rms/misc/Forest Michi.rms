/* LAND_GENERATION: Creates large terrain areas including player starting areas */
<LAND_GENERATION>
base_terrain WOODIES						/* Sets default terrain covering entire map */
/* If Gametype is King of the Hill, create a center island for Wonder */
if KING_OT_HILL								
create_land 
{
	terrain_type 				LAYER_A		/* Set the Terrain type for this land to LAYER_A */
	land_percent 			(LENGTH / 20)	/* land mass (LENGTH/20) */
	land_position 					50 50	/* Land located in center of map (50% 50%) */
	other_zone_avoidance_distance 	12		/* Minimum 12-tile gap between different zones */
	clumping_factor 				30		/* How much tiles prefer to be together next to other tiles of the same clump. */
	zone 							13		/* Land zone set to 13 */
	land_id 						13		/* Land id set to 13 */
}
else
endif
create_player_lands 						/* Creates a land for every player with specified parameters */
{
	generate_mode 1
	terrain_type	            LAYER_A  	/* Terrain type for player starting clearings */
	set_zone_by_team              			/* Team members share same zone - their lands can grow to touch each other */
	if LENGTH > 220							/* If mapsize is larger than LARGE, adjust map scaling */
	land_percent    	    		60  	/* 60% land mass */
	circle_radius 					45 1	/* Positions players in circle: 45% radius from center, 1 variance */
	base_size                     	24    	/* 24-tile square land origin size */
	other_zone_avoidance_distance 	20    	/* 20-tile distance between player zones */
	elseif LENGTH 220                       /* If mapsize is LARGE, adjust map scaling */
	land_percent    	    		50  	/* 50% land mass */
	circle_radius 					44 1	/* Positions players in circle: 44% radius from center, 1 variance */
	base_size 						22		/* 22-tile square land origin size */
	other_zone_avoidance_distance 	18    	/* 18-tile distance between player zones */
	elseif LENGTH 200                       /* If mapsize is NORMAL, adjust map scaling */
	land_percent    	    		45  	/* 45% land mass */
	circle_radius 					42 1	/* Positions players in circle: 42% radius from center, 1 variance */
	base_size                     	20    	/* 20-tile square land origin size */
	other_zone_avoidance_distance 	16    	/* 16-tile distance between player zones */
	elseif LENGTH 168                       /* If mapsize is MEDIUM, adjust map scaling */
	land_percent    	    		40  	/* 40% land mass */  
	circle_radius 					41 1	/* Positions players in circle: 41% radius from center, 1 variance */
	base_size                     	18    	/* 18-tile square land origin size */
	other_zone_avoidance_distance 	14    	/* 14-tile distance between player zones */
	elseif LENGTH 144                     	/* If mapsize is SMALL, adjust map scaling */
	land_percent    	    		35  	/* 35% land mass */  
	circle_radius 					40 1   	/* Positions players in circle: 40% radius from center, 1 variance */
	base_size                     	16    	/* 16-tile square land origin size */
	other_zone_avoidance_distance 	12    	/* 12-tile distance between player zones */
	elseif LENGTH 120                    	/* If mapsize is TINY, adjust map scaling */
	land_percent    	    		30	  	/* 30% land mass */  
	circle_radius 					39 1   	/* Positions players in circle: 39% radius from center, 1 variance */
	base_size                     	12    	/* 12-tile square land origin size */
	other_zone_avoidance_distance 	10    	/* 10-tile distance between player zones */
	elseif LENGTH < 120                     /* If mapsize is less than TINY, adjust map scaling */
	land_percent    	    		25 		/* 25% land mass */  
	circle_radius 					46 0   	/* Positions players in circle: 46% radius from center, 0 variance */
	base_size                     	8    	/* 8-tile square land origin size */
	other_zone_avoidance_distance 	8    	/* 8-tile distance between player zones */
	endif
	set_circular_base             			/* Makes land origin circular instead of square */
	clumping_factor 			rnd(4,8)	/* How much tiles prefer to be together next to other tiles of the same clump. */
}	

/* ELEVATION_GENERATION: Create hills */
<ELEVATION_GENERATION>
create_elevation 6 								/* Sets max height allowed in create_elevation (0-16) */
{
	base_terrain 				LAYER_A			/* Terrain type to apply elevation */
	set_scale_by_size							/* Scales tile count by map size */
	number_of_tiles 			2048			/* Amount of elevated tiles */
	number_of_clumps 			(LENGTH / 4)	/* Amount of hills LENGTH divided by 4 */
	enable_balanced_elevation					/* To keep elevation balanced across entirety of map */
}


<LAND_GENERATION>
base_terrain VODA						/* Sets default terrain covering entire map */
create_land 
{
	terrain_type LAYER_A
	land_percent 				50
	land_position 				52 50
	if LENGTH > 200
		left_border 			34
		right_border 			34	
		top_border 				38
		bottom_border 			38
	elseif LENGTH 200
		left_border 			32
		right_border 			32	
		top_border 				36
		bottom_border 			36	
	elseif LENGTH < 200
		left_border 			30
		right_border 			30
		top_border 				32
		bottom_border 			32
	endif
	border_fuzziness 			60
	land_id 					11		/* Land ID for this create_land */
	zone 						11		/* Zone for this create_land */
}
